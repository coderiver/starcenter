include ../../_mixins
.filter
  label.filter__item
    input(type="radio", name="buy", value="all", checked)
    .btn.btn_transparent
      .btn__front
        | All Properties
      .btn__hover
        .btn__hover-in
          | All Properties
  label.filter__item
    input(type="radio", name="buy", value="commercial")
    .btn.btn_transparent
      .btn__front
        | Commercial Property
      .btn__hover
        .btn__hover-in
          | Commercial Property
  label.filter__item
    input(type="radio", name="buy", value="residential")
    .btn.btn_transparent
      .btn__front
        | Residential Property
      .btn__hover
        .btn__hover-in
          | Residential Property

ul.filter-container
  li.object-wrapper(data-filter="commercial")
    .object.js-box
      .object__inner

        .object__gallery

          .object__buttons
            button.object__slider-prev(type="button")
              span
                +icon("arrow")
                |  Prev photo
            button.object__slider-next(type="button")
              span
                | Next photo
                +icon("arrow")
            button.object__close(type="button")
              span
                | Minimize object
                +icon("arrow-sm")

          .object__slider
            .object__slide(style="background-image:url('img/objects/1.jpg')")
            .object__slide(style="background-image:url('img/objects/1.1.jpg')")
            .object__slide(style="background-image:url('img/objects/1.2.jpg')")
            .object__slide(style="background-image:url('img/objects/1.3.jpg')")
            .object__slide(style="background-image:url('img/objects/1.4.jpg')")
            .object__slide(style="background-image:url('img/objects/1.5.jpg')")
            .object__slide(style="background-image:url('img/objects/1.6.jpg')")

          .info.table
            .table__border
              include ../../../img/svg/svg-borders/optimized/info-table.svg
            .info__inner
              .info__cell.info__cell1
                a.btn.btn_object(href="#")
                  .btn__hover
                    .btn__hover-in
                      | Краснозвездный 56
                      br
                      small Киев, прсп. Краснозвездный, 56
                  .btn__front
                    | Краснозвездный 56
                    br
                    small Киев, прсп. Краснозвездный, 56
              .info__cell.info__cell3
                .info-item
                  small.info-item__label Area
                  | 13 713 М<sup>2</sup>
              .info__cell.info__cell2
                .info-item
                  small.info-item__label Completion year
                  | 2015
              .info__cell.info__cell2
                .info-item
                  small.info-item__label Total floors
                  | 13
              .info__cell.info__cell3
                .info-item
                  small.info-item__label Free area
                  | 7 800 М<sup>2</sup>

        .object__detail
          h4.heading.heading_red
            | Special offer
          p.
            Now there is a 25% discount on the first year of the
            lease size upon signing the contract before the summer

          a.btn.btn_link(href="#")
            span.btn__hover(data-hover="Download presentation")
            span Download presentation

